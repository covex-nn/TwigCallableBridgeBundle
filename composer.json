{
    "name" : "covex-nn/twig-callable-bridge-bundle",
    "description" : "Provides a simple interface to extend Twig with PHP functions",
    "type" : "symfony-bundle",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Andrey F. Mindubaev",
            "email" : "covex.mobile@gmail.com"
        }
    ],
    "require" : {
        "php" : ">=5.3.0",
        "symfony/twig-bundle" : "~2.3"
    },
    "require-dev" : {
        "symfony/symfony" : "~2.3",
        "symfony/monolog-bundle" : "~2.3",
        "apnet/functional-test-bundle" : "~1.1,>=1.1.3",
        "phpunit/phpunit" : "~4.0",
        "apnet/coding-standard" : "~2.0",
        "knplabs/knp-markdown-bundle" : "~1.3"
    },
    "extra" : {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability" : "beta",
    "autoload" : {
        "psr-0" : {
            "Covex\\" : "src/"
        }
    },
    "config" : {
        "bin-dir" : "bin"
    }
}
